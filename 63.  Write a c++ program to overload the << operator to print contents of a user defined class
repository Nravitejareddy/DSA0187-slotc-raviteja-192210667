#include <iostream>
#include <string>

class MyClass {
private:
    std::string data;

public:
    // Constructor
    MyClass(const std::string& initData) : data(initData) {
    }

    // Overloading the << operator as a friend function
    friend std::ostream& operator<<(std::ostream& os, const MyClass& myObj) {
        os << "Data: " << myObj.data;
        return os;
    }
};

int main() {
    MyClass myObject("Hello, World!");

    // Using the overloaded << operator to print the contents
    std::cout << myObject << std::endl;

    return 0;
}

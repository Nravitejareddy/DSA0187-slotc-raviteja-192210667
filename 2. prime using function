#include<iostream>
#include<cmath>

int main() {
    int n, i;
    std::cout << "Enter a number: ";
    std::cin >> n;

    if (n < 2) {
        std::cout << "Not prime" << std::endl;
        return 0;
    }
    for (i = 2; i * i <= n; i++) {
        if (n % i == 0) {
            std::cout << "Not prime" << std::endl;
            return 0;
        }
    }
    std::cout << "Prime" << std::endl;
    return 0;
}
